<h1>Dating App</h1>
<% if current_user %>
  Logged in as <%= current_user.email %>. <%= link_to "Edit", edit_user_path(current_user) %>
  <%= render 'users' %>
  <%= link_to "Log Out", logout_path %>
<% else %>
  <h2>You are not logged in!</h2>
  <%= link_to "Sign Up", signup_path %> or 
  <%= link_to "Log In", login_path %>
<% end %>
<p id="notice">
  <%= notice %>
</p>
